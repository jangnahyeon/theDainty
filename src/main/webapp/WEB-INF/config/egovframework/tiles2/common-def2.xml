		<put-attribute name="footer" value="/WEB-INF/jsp/tiles2/footer.jsp" />
	</definition>
	
	<definition name="frontDsnStd" template="/WEB-INF/jsp/tiles2/layout.jsp">
		<put-attribute name="header" value="/WEB-INF/jsp/tiles2/header.jsp" />
		<put-attribute name="subTabmenu" value="/WEB-INF/jsp/tiles2/dsnStd/subTabmenu.jsp" />
		<put-attribute name="title" value="/WEB-INF/jsp/tiles2/title.jsp" />
		<put-attribute name="footer" value="/WEB-INF/jsp/tiles2/footer.jsp" />
	</definition>
	
