	<definition name="frontContent" template="/WEB-INF/jsp/tiles2/layout.jsp">
		<put-attribute name="header" value="/WEB-INF/jsp/tiles2/header.jsp" />
		<put-attribute name="subTabmenu" value="/WEB-INF/jsp/tiles2/content/subTabmenu.jsp" />
