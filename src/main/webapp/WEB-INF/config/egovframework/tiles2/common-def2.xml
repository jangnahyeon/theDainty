	<definition name="/front2/*" extends="frontMain">
		<put-attribute name="content"
			value="/WEB-INF/jsp/front2/{1}.jsp" />
	</definition> 
	
	<definition name="/front2/dsnStd/*" extends="frontDsnStd">
		<put-attribute name="content"
			value="/WEB-INF/jsp/front2/dsnStd/{1}.jsp" />
	</definition>
		
	<definition name="/front2/dsnBank/*" extends="frontDsnBank">
		<put-attribute name="content"
			value="/WEB-INF/jsp/front2/dsnBank/{1}.jsp" />
