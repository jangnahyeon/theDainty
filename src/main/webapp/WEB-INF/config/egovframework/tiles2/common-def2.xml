        <put-attribute name="adminLeft" value="/WEB-INF/jsp/tiles2/adminSisulMagLeft.jsp" />
        <put-attribute name="adminInnerTitle" value="/WEB-INF/jsp/tiles2/adminInnerTitle.jsp" />
