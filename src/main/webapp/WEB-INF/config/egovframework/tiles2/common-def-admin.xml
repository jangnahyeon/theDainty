<!DOCTYPE tiles-definitions PUBLIC
"-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>    
    <definition name="adminLayout" template="/WEB-INF/jsp/dlink_new/tiles/admin/adminLayout.jsp">
        <put-attribute name="adminHeader" 		value="/WEB-INF/jsp/dlink_new/tiles/admin/adminHeader.jsp" />
        <put-attribute name="adminLeft" 		value="/WEB-INF/jsp/dlink_new/tiles/admin/adminMagLeft.jsp" />
        <put-attribute name="adminInnerTitle" 	value="/WEB-INF/jsp/dlink_new/tiles/admin/adminInnerTitle.jsp" />
        <put-attribute name="adminFooter" 		value="/WEB-INF/jsp/dlink_new/tiles/admin/adminFooter.jsp" />
    </definition>	
	
	<definition name="/admin/code/*" extends="adminLayout">
	    <put-attribute name="content"
	        value="/WEB-INF/jsp/dlink_new/admin/code/{1}.jsp" />
	</definition>
	
